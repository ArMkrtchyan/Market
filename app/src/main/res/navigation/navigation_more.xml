<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_more"
    app:startDestination="@id/moreFragment">

    <fragment
        android:id="@+id/moreFragment"
        android:name="com.armboldmind.grandmarket.ui.more.MoreFragment"
        android:label="fragment_more"
        tools:layout="@layout/fragment_more">
        <action
            android:id="@+id/action_moreFragment_to_requestedProductsFragment"
            app:destination="@id/requestAProductFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_moreFragment_to_notificationsFragment"
            app:destination="@id/notificationsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_moreFragment_to_FAQFragment"
            app:destination="@id/FAQFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_moreFragment_to_subscriptionsFragment"
            app:destination="@id/subscriptionsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_moreFragment_to_settingsFragment"
            app:destination="@id/settingsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_moreFragment_to_requestsFragment"
            app:destination="@id/requestsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_moreFragment_to_privacyPolicyFragment"
            app:destination="@id/privacyPolicyFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_moreFragment_to_personalInformationFragment"
            app:destination="@id/personalInformationFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_moreFragment_to_ordersFragment"
            app:destination="@id/ordersFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_moreFragment_to_newsAndEventsFragment"
            app:destination="@id/newsAndEventsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_moreFragment_to_contactUsFragment"
            app:destination="@id/contactUsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_moreFragment_to_addressesFragment"
            app:destination="@id/addressesFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_moreFragment_to_cardsFragment"
            app:destination="@id/cardsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_moreFragment_to_aboutUsFragment"
            app:destination="@id/aboutUsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_moreFragment_to_aboutAppFragment"
            app:destination="@id/aboutAppFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/requestAProductFragment"
        android:name="com.armboldmind.grandmarket.ui.more.requests.RequestAProductFragment"
        android:label="RequestAProductFragment"
        tools:layout="@layout/fragment_request_a_product">
    </fragment>
    <fragment
        android:id="@+id/notificationsFragment"
        android:name="com.armboldmind.grandmarket.ui.more.notifications.NotificationsFragment"
        android:label="fragment_notifications"
        tools:layout="@layout/fragment_notifications">
        <action
            android:id="@+id/action_notificationsFragment_to_subscriptionDetailsFragment"
            app:destination="@id/subscriptionDetailsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_notificationsFragment_to_requestDetailsFragment"
            app:destination="@id/requestDetailsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_notificationsFragment_to_orderDetailsFragment"
            app:destination="@id/orderDetailsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_notificationsFragment_to_newsDetailsFragment"
            app:destination="@id/newsDetailsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/FAQFragment"
        android:name="com.armboldmind.grandmarket.ui.more.support.FAQFragment"
        android:label="FAQFragment"
        tools:layout="@layout/fragment_faq" />
    <fragment
        android:id="@+id/aboutAppFragment"
        android:name="com.armboldmind.grandmarket.ui.more.informative.AboutAppFragment"
        android:label="fragment_about_app"
        tools:layout="@layout/fragment_about_app" />
    <fragment
        android:id="@+id/aboutUsFragment"
        android:name="com.armboldmind.grandmarket.ui.more.informative.AboutUsFragment"
        android:label="fragment_about_us"
        tools:layout="@layout/fragment_about_us" />
    <fragment
        android:id="@+id/addAddressFragment"
        android:name="com.armboldmind.grandmarket.ui.more.addresses.AddAddressFragment"
        android:label="fragment_add_address"
        tools:layout="@layout/fragment_add_address">
        <argument
            android:name="address"
            app:argType="com.armboldmind.grandmarket.data.models.domain.Address"
            app:nullable="true" />
        <argument
            android:name="listSize"
            android:defaultValue="0"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/addCardFragment"
        android:name="com.armboldmind.grandmarket.ui.more.cards.AddCardFragment"
        android:label="fragment_add_card"
        tools:layout="@layout/fragment_add_card" />
    <fragment
        android:id="@+id/cardsFragment"
        android:name="com.armboldmind.grandmarket.ui.more.cards.CardsFragment"
        android:label="fragment_cards"
        tools:layout="@layout/fragment_cards">
        <action
            android:id="@+id/action_cardsFragment_to_addCardFragment"
            app:destination="@id/addCardFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/addressesFragment"
        android:name="com.armboldmind.grandmarket.ui.more.addresses.AddressesFragment"
        android:label="fragment_addresses"
        tools:layout="@layout/fragment_addresses">
        <action
            android:id="@+id/action_addressesFragment_to_addAddressFragment"
            app:destination="@id/addAddressFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/contactUsFragment"
        android:name="com.armboldmind.grandmarket.ui.more.support.ContactUsFragment"
        android:label="fragment_contact_us"
        tools:layout="@layout/fragment_contact_us" />
    <fragment
        android:id="@+id/newsAndEventsFragment"
        android:name="com.armboldmind.grandmarket.ui.more.informative.NewsAndEventsFragment"
        android:label="fragment_news_and_events"
        tools:layout="@layout/fragment_news_and_events">
        <action
            android:id="@+id/action_newsAndEventsFragment_to_newsDetailsFragment"
            app:destination="@id/newsDetailsFragment"
            app:enterAnim="@anim/fade_in_500"
            app:exitAnim="@anim/fade_out_500"
            app:popEnterAnim="@anim/fade_in_500"
            app:popExitAnim="@anim/fade_out_500" />
    </fragment>
    <fragment
        android:id="@+id/ordersFragment"
        android:name="com.armboldmind.grandmarket.ui.more.orders.OrdersFragment"
        android:label="fragment_orders"
        tools:layout="@layout/fragment_orders">
        <action
            android:id="@+id/action_ordersFragment_to_orderDetailsFragment"
            app:destination="@id/orderDetailsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/personalInformationFragment"
        android:name="com.armboldmind.grandmarket.ui.more.personalInformation.PersonalInformationFragment"
        android:label="fragment_personal_information"
        tools:layout="@layout/fragment_personal_information">
        <action
            android:id="@+id/action_personalInformationFragment_to_changeLoginInfoFragment"
            app:destination="@id/changeLoginInfoFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/privacyPolicyFragment"
        android:name="com.armboldmind.grandmarket.ui.more.informative.PrivacyPolicyAndTermsFragment"
        android:label="fragment_privacy_policy"
        tools:layout="@layout/fragment_privacy_policy_and_terms">
        <argument
            android:name="removePaddingBottom"
            android:defaultValue="false"
            app:argType="boolean" />
        <argument
            android:name="isTerms"
            android:defaultValue="true"
            app:argType="boolean" />
    </fragment>
    <fragment
        android:id="@+id/requestsFragment"
        android:name="com.armboldmind.grandmarket.ui.more.requests.RequestsFragment"
        android:label="fragment_requests"
        tools:layout="@layout/fragment_requests">
        <action
            android:id="@+id/action_requestsFragment_to_requestDetailsFragment"
            app:destination="@id/requestDetailsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_requestsFragment_to_requestAProductFragment"
            app:destination="@id/requestAProductFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.armboldmind.grandmarket.ui.more.settings.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings">
        <action
            android:id="@+id/action_settingsFragment_to_languageSettingsFragment"
            app:destination="@id/languageSettingsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_settingsFragment_to_notificationsSettingsFragment"
            app:destination="@id/notificationsSettingsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/subscriptionsFragment"
        android:name="com.armboldmind.grandmarket.ui.more.subscribtions.SubscriptionsFragment"
        android:label="fragment_subscriptions"
        tools:layout="@layout/fragment_subscriptions">
        <action
            android:id="@+id/action_subscriptionsFragment_to_subscriptionDetailsFragment"
            app:destination="@id/subscriptionDetailsFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/newsDetailsFragment"
        android:name="com.armboldmind.grandmarket.ui.more.informative.NewsDetailsFragment"
        android:label="fragment_news_details"
        tools:layout="@layout/fragment_news_details">
        <argument
            android:name="newsAndEvent"
            android:defaultValue="@null"
            app:argType="com.armboldmind.grandmarket.data.models.domain.NewsAndEvents"
            app:nullable="true" />
        <argument
            android:name="id"
            android:defaultValue="0L"
            app:argType="long" />
    </fragment>
    <fragment
        android:id="@+id/notificationsSettingsFragment"
        android:name="com.armboldmind.grandmarket.ui.more.settings.NotificationsSettingsFragment"
        android:label="NotificationsSettingsFragment"
        tools:layout="@layout/fragment_notifications_settings" />
    <fragment
        android:id="@+id/languageSettingsFragment"
        android:name="com.armboldmind.grandmarket.ui.more.settings.LanguageSettingsFragment"
        android:label="LanguageSettingsFragment"
        tools:layout="@layout/fragment_language_settings" />
    <fragment
        android:id="@+id/changeLoginInfoFragment"
        android:name="com.armboldmind.grandmarket.ui.more.personalInformation.ChangeLoginInfoFragment"
        android:label="ChangeLoginInfoFragment"
        tools:layout="@layout/fragment_change_login_info">
        <action
            android:id="@+id/action_changeLoginInfoFragment_to_verifyLoginInfoFragment"
            app:destination="@id/verifyLoginInfoFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <argument
            android:name="isEmailChoosed"
            android:defaultValue="false"
            app:argType="boolean" />
    </fragment>
    <fragment
        android:id="@+id/verifyLoginInfoFragment"
        android:name="com.armboldmind.grandmarket.ui.more.personalInformation.VerifyLoginInfoFragment"
        android:label="VerifyLoginInfoFragment"
        tools:layout="@layout/fragment_verify_login_info">
        <argument
            android:name="phone"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="email"
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/orderDetailsFragment"
        android:name="com.armboldmind.grandmarket.ui.more.orders.OrderDetailsFragment"
        android:label="OrderDetailsFragment"
        tools:layout="@layout/fragment_order_details">
        <argument
            android:name="id"
            android:defaultValue="0L"
            app:argType="long" />
    </fragment>
    <fragment
        android:id="@+id/requestDetailsFragment"
        android:name="com.armboldmind.grandmarket.ui.more.requests.RequestDetailsFragment"
        android:label="RequestDetailsFragment"
        tools:layout="@layout/fragment_request_details">
        <argument
            android:name="request"
            app:argType="com.armboldmind.grandmarket.data.models.domain.RequestProduct"
            app:nullable="false" />
    </fragment>
    <fragment
        android:id="@+id/subscriptionDetailsFragment"
        android:name="com.armboldmind.grandmarket.ui.more.subscribtions.SubscriptionDetailsFragment"
        android:label="SubscriptionDetailsFragment"
        tools:layout="@layout/fragment_subscription_details">
        <argument
            android:name="id"
            android:defaultValue="0L"
            app:argType="long" />
    </fragment>
    <fragment
        android:id="@+id/requestProductInfoFragment"
        android:name="com.armboldmind.grandmarket.ui.more.requests.RequestProductInfoFragment"
        android:label="RequestProductInfoFragment"
        tools:layout="@layout/fragment_request_product_info">

        <action
            android:id="@+id/action_requestProductInfoFragment_to_requestAProductFragment"
            app:destination="@id/requestAProductFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
</navigation>